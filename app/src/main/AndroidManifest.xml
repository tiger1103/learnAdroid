<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ButtonActivity" />
        <activity android:name=".EditTextActivity" />
        <activity android:name=".RadioButtonActivity" />
        <activity android:name=".CheckBoxActivity" />
        <activity android:name=".ImageViewActivity" />
        <activity android:name=".listview.ListViewActivity" />
        <activity android:name=".gridview.GridViewActivity" />
        <activity android:name=".recycleview.RecycleViewActivity" />
        <activity android:name=".recycleview.LineaRecycleViewActivity" />
        <activity android:name=".recycleview.HorRecycleViewActivity" />
        <activity android:name=".recycleview.GridRecycleViewActivity" />
        <activity android:name=".recycleview.PuRecycleViewActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".ToastViewActivity" />
        <activity android:name=".UIActivity"></activity>
    </application>

</manifest>